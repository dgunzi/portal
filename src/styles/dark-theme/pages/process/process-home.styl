.dealWorkContainer
  width 97%
  margin 3px auto 8px
  padding-bottom 10px
  background-color #082233
  border-top: 2px solid #217294
  border-radius 0 0 2px 2px
  box-shadow 0 0 2px #125159 inset
  .title
    border-bottom 1px solid border_color
    .titleInfo
      line-height 30px
      color baseColor
      &:first-child
        margin-left 15px
    .attendedfull
        color ctx_menu_hover_bd
    .attended
        color red_l2_black
    .type
      padding: 1px 6px;
      display: inline-block;
      position: relative;
      top: 5px;
      right: -2px;
      font-size: 12px;
      border-radius: 2px;
    .problemOrderColor /*问题工单*/
      color: #D8F7EA;
      background-color: rgba(31, 154, 85, 0.6);
      box-shadow: 0 0 10px #062D1A, 0 0 1px #7FE495 inset;
    .knowledgeOrderColor /*知识工单*/
      color: #E5E0E8;
      background-color: rgba(97, 95, 111, 0.6);
      box-shadow: 0 0 10px #2E2C2F, 0 0 1px #AC9ABD inset;
    .eventOrderColor /*事件工单*/
      color: #F7D8D8;
      background-color: rgba(176, 28, 22, 0.6);
      box-shadow: 0 0 10px #480C0C, 0 0 1px #E47F7F inset;
    .changeOrderColor /*变更工单*/
      color: #F7F6D8;
      background-color: rgba(206, 189, 6, 0.4);
      box-shadow: 0 0 10px #211e03, 0 0 1px #f3f5a2 inset;
  .deil
    padding 0 15px
    & span
      font-size 14px
      line-height 20px
      color title_1
    .status
      display inline-block
      width 99%
      font-weight bold
      font-size 12px
    .green
      color #15B757
    .summary
      display inline-block
      width 99%
      white-space nowrap
      text-overflow ellipsis
      overflow hidden
      vertical-align top
      margin-left 5px
      min-height 20px
      font-size 12px
  .info
    padding 0 15px
    color darkWordColor
    display: flex
    align-items: flex-start
    justify-content: space-between
    & p
      white-space nowrap
      text-overflow ellipsis
      overflow hidden
    & span
      flex 1
      font-size 12px
      color darkWordColor
      line-height 22px
      white-space nowrap
      text-overflow ellipsis
      overflow hidden
    & i
      position relative
      top -2px
  .btns
    background none
    padding 3px 15px
    padding-bottom 0
    .s-badge + .s-badge
      margin-left 25px
    .badgeCol
      width 44%
    .badgeCol.center
      display block
      margin 0 auto
      width 100%
    .btnCol
      display inline-block
      width 100%
      margin-right 4%
      background-image linear-gradient(#065D71, #064C6B)
      border 1px solid #00CAF4
      transition all .1s
      &:hover
        background-image linear-gradient(#04b1e0, #04b1e0)
      &:last-child
        margin-right 0
      & i
        position relative
        top 1px
    .is-disabled
      border-color #17546F
      background #102D40
      &:hover
        border-color #17546F
        background #102D40
