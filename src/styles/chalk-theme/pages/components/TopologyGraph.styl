.topology_canvas
  position absolute
  top 0px
  left 0px
  height 585px
  width 100%
  overflow hidden
  background-size 100% 100%
.cellTips
  border 1px solid border_color
  background-color title_2
  height auto
  position absolute
  font-size 12px
  top 0px
  left 0px
  .clr
    clear both
  .item
    line-height 30px
    margin-top 6px
    height 30px
    font-weight bold
    i
      margin-left 15px
    span
      margin-left 5px
  .line
    line-height 30px
    margin-bottom 2px
    margin-left 10px
    margin-right 10px
    background-color normalcy_text_2
    color gray_l3_text
  .line:first-child
    margin-top 10px
  .line:last-child
    margin-bottom 12px
  .title
    display inline-block
    width 70px
    text-align center
    background-color title_2
  .info
    padding-left 5px
