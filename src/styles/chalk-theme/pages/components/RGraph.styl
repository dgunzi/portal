#relationgraph
  table
    width 100% !important
  .canvas_container
    width 100%
    position relative
    text-align left
    height 100%
    overflow hidden
  #cmdbViz-canvaswidget
    width 100%
    position relative
    height 100%
    overflow hidden
  .assetElement
    cursor pointer
    margin 0px 0px 0px 0px
    padding 0px 0px
    width 120px
    height 80px
    position absolute
    left 30px
    top 30px
    font-size 100%
    z-index 99
    display block !important
    img
      &.icon
        display inline
        width 40%
        margin-left -20%
        position absolute
        left 50%
        top -5%
        z-index 99
      &.attr-left
        position absolute
        width 10%
        top 65%
        z-index 99
        margin-left -10%
        left 0px
    &:hover
      .node-more-info
        display block
  .node-assetContainer
    position absolute
    width 100%
    height 100%
    bottom 23%
    right 1%
    &:hover
      z-index 9999999 !important
  .jit-assetnode
    display inline
    width 100%
    margin 2% 0%
    position absolute
    text-align center
    top 52%
    span
      display block
      margin 0
      padding 1% 0 1% 0
      &.jit-ciname
        font-size 100%
        color title_2
        padding 2% 1%
        font-size 110%
        .floatleft
          float left
      &.jit-citype
        font-size 80%
        color gray_l2_text
        display none
  .jit-assetnode-inner
    padding 4% 0%
    border-radius 0.4em
  .bg_white_node
    background-color original_color
  .jit-assetnode:hover, .assetElement.node-active .jit-assetnode
    color normalcy_text
    border-color border_color

  .node-active
    .jit-relnode
      color link_text
    .bg_white_node
      background-color original_l1_white !important
  .node-more-info
    position absolute
    right 3px
    top 2px
    display none
    width 16px
    height 16px
    cursor pointer
    z-index 9999
  .node-attrinfo
    position absolute
    left 3px
    top 3px
    width 16px
    height 18px
    visibility hidden
    cursor pointer
  .node-relattr
    .node-attrinfo
      visibility visible
  .labelElement
    overflow visible
    margin 0 0 0 0
    padding 0 0 0 0
    font-size 100%
    cursor pointer
  .jit-relnode
    text-align center
    color gray_l2_text
    background none
    float left
    margin-top 2%
    font-size 0.9em
    padding 0 0 0 0
    text-transform capitalize
    width auto
  .info_critical
    display block
    position absolute
    top 28%
    right 25%
    display none
    z-index 100
    width 18%
  .ui-bt-expand
    position absolute
    right -20px
    width 20px
    height 100%
    top 0
    .expand_icon
      position absolute
      right -9px
      top 50%
      margin-top -6px
      width 9px
      height 9px
  .nocirelmsg
    left 25%
  .graphMenu
    position relative
    top 8px
    left 8px
    z-index 8
    ul
      &.sf-menu
        background none
  .graphactions
    height 20px
    padding-top 15px
    position absolute
    right 23px
    z-index 8
    display block
  .graphactionspop
    margin-left 450px
    margin-top 8px
    z-index 9999
  #asset_desc_panel
    position absolute
    text-align left
    left 0px
    top 0px
    display none
    height auto
    opacity 0
    z-index 99999
    margin-left 0px
  .description_panel
    width 330px
    display block
    border 1px solid border_color
    -webkit-box-shadow none
    -moz-box-shadow none
    box-shadow none
    z-index 9999999
    -moz-border-radius 0.4em
    -webkit-border-radius 0.4em
    border-radius 0.4em
    background #dbdfe6


  .desc_button_panel
    padding 5px
    background original_l2_white
  .desc_title
    color main_text
    padding 5px 10px 15px 0px
    position relative
    margin-left 2px
    h2
      color original_l3_black
      padding 2px 0px 2px 10px
      margin 0px
      font-size 16px
      font-weight normal
    h3
      color main_text
      padding 2px 0px
      margin 0px
      font-size 13px
    img
      border 0px
      float right
    a
      &.desc-title-link
        color gray_l1_text
        font-size 13px
        font-weight bold
        display block
        padding 4px 0px 4px 10px
        margin 0px 55px 0px 0px
        text-decoration none
        &:hover
          background main_text
    .ui-close
      position absolute
      right 10px
      top 13px
      cursor pointer
    .add-label
      line-height 35px
      text-align left
      margin-left 10px
    .s-radio
      display block
      line-height 20px
      width 555px
      border 1px solid border_color
      border-bottom 0px
      &:last-child
        border-bottom 1px solid border_color
      & + .s-radio
        margin-left 0px
    .s-radio__input
      margin-left 10px
    .s-radio__inner
      margin-top 10px
    .s-radio__label
      margin-left 10px
